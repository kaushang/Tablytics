{
  "manifest_version": 3,
  "name": "Tablytics",
  "version": "1.0",
  "description": "Tab Time Tracker",
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "notifications"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}